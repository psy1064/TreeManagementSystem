<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PriceCheckActivity">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="207dp"
        android:layout_height="77dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="55dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/rect"
        android:gravity="center"
        android:text="원가 확인"
        android:textSize="36sp" />

    <TableLayout
        android:id="@+id/tablelayout"
        android:layout_width="1187dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/rect"
        android:stretchColumns="*">

        <TableRow

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/checkA_dateTitleText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="재고등록일"
                android:textSize="36sp" />

            <TextView
                android:id="@+id/checkA_standardTitleText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="규 격"
                android:textSize="36sp" />

            <TextView
                android:id="@+id/checkA_countTitleText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="수 량"
                android:textSize="36sp" />

            <TextView
                android:id="@+id/checkA_priceTitleText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="원 가"
                android:textSize="36sp" />
        </TableRow>


    </TableLayout>

</RelativeLayout>